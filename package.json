{
  "name": "video-transcoder-s3",
  "version": "1.0.0",
  "type": "module",
  "description": "A video transcoder project that uploads videos to AWS S3 and processes them.",
  "main": "src/index.js",
  "scripts": {
    "start": "node --env-file=.env src/index.js",
    "dev": "nodemon --env-file=.env src/index.js",
    "worker": "node --env-file=.env src/workers/transcodeWorker.js",
    "worker:dev": "nodemon --env-file=.env src/workers/transcodeWorker.js",
    "webhook": "node --env-file=.env automate.js",
    "webhook:dev": "nodemon --env-file=.env automate.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.520.0",
    "@aws-sdk/lib-storage": "^3.948.0",
    "aws-sdk": "^2.1234.0",
    "bullmq": "^5.66.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ioredis": "^5.8.2",
    "mongoose": "^9.0.1",
    "multer": "^1.4.2",
    "multer-s3": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "author": "",
  "license": "ISC"
}